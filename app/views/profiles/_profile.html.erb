<div id="<%= dom_id profile %>">
  <p>
    <strong>Fname:</strong>
    <%= profile.fname %>
  </p>

  <p>
    <strong>Lname:</strong>
    <%= profile.lname %>
  </p>

  <p>
    <strong>Tag:</strong>
    <%= profile.tag %>
  </p>

  <p>
    <strong>School:</strong>
    <%@schoolname = School.find_by(id: profile.school_id)%>
    <%= @schoolname.name %>
  </p>

  <p>
    <strong>Status:</strong>
    <%= profile.status %>
  </p>

</div>
